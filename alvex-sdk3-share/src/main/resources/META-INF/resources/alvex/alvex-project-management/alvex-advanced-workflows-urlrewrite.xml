<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite use-query-string="true">
	<rule>
		<from>^/page/alvex/my-tasks(.*)</from>
		<to>/page/alvex-tasks-view$1</to>
	</rule>
	<rule>
		<from>^/page/alvex/task-edit(.*)</from>
		<to>/page/alvex-task-page$1</to>
	</rule>
	<rule>
		<from>^/page/alvex/task-details(.*)</from>
		<to>/page/alvex-task-page$1</to>
	</rule>
	<rule>
		<from>^/page/alvex/workflow-details(.*)</from>
		<to>/page/alvex-task-page$1</to>
	</rule>
	<rule>
		<from>^/page/alvex/site/(.*)/task-edit(.*)</from>
		<to>/page/site/$1/alvex-task-page$2</to>
	</rule>
	<rule>
		<from>^/page/alvex/site/(.*)/task-details(.*)</from>
		<to>/page/site/$1/alvex-task-page$2</to>
	</rule>
	<rule>
		<from>^/page/alvex/site/(.*)/workflow-details(.*)</from>
		<to>/page/site/$1/alvex-task-page$2</to>
	</rule>
</urlrewrite>